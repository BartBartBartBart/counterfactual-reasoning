srun python eval_GPT_letterstring.py --gen gen --model "Qwen/Qwen3-8B" --num_permuted 5 --promptstyle analogical --extra-split
srun python eval_GPT_letterstring.py --gen gen --model "Qwen/Qwen3-8B" --num_permuted 10 --promptstyle analogical --extra-split
srun python eval_GPT_letterstring.py --gen gen --model "Qwen/Qwen3-8B" --num_permuted 20 --promptstyle analogical --extra-split
srun python eval_GPT_letterstring.py --gen gen --model "Qwen/Qwen3-8B" --num_permuted symb --promptstyle analogical --extra-split
